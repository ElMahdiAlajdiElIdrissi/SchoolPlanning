<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import java.net.URL?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="app.controllers.StartupController"
          fx:id="group" styleClass="root">

    <stylesheets>
        <URL value="@css/Styling.css"/>
    </stylesheets>

    <HBox spacing="10">
        <Button styleClass="button" id="studentButton" fx:id="student" text="Student" prefWidth="75" onAction="#viewStudent"/>
        <Button styleClass="button" id="docentButton" fx:id="teacher" text="Docent" prefWidth="75" onAction="#viewTeacher"/>
    </HBox>
</GridPane>
