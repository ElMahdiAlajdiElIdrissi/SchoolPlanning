<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

    <VBox xmlns:fx="http://javafx.com/fxml" fx:controller="App.controllers.TeacherScheduleController">
        <Text text="Welcome"/>
        <BorderPane>
            <left>
                <VBox spacing="5">
                    <Button fx:id="assignedCourse" text="Assigned Courses" maxWidth="200"
                            onAction="#assignedCourses"/>
                    <Button fx:id="enlistedStudent" text="Enlisted Students" maxWidth="200"
                            onAction="#enlistedStudents"/>
                    <Button fx:id="logout" text="Logout" maxWidth="200"
                            onAction="#logout"/>
                </VBox>
            </left>
            <center>
                <ScrollPane>
                    <content>
                        <ImageView>
                            <Image url="@css/backGround.jpg"/>

                        </ImageView>
                    </content>
                </ScrollPane>
            </center>
        </BorderPane>
    </VBox>

