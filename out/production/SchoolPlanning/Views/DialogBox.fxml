<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<Group xmlns="http://javafx.com/javafx"
       xmlns:fx="http://javafx.com/fxml"
       fx:controller="DialogBoxController">


    <stylesheets>
        <URL value="@DialogBoxStyling.css"/>
    </stylesheets>

    <VBox centerShape="true" layoutX="75" layoutY="50" spacing="20">
        <Label text="Login Succesful!"/>
    </VBox>

    <HBox layoutX="150" layoutY="100" spacing="10">
        <Button fx:id="confirmButton" text="OK" prefWidth="75" onAction="#okAction"/>
    </HBox>
</Group>
